  "RapidKit FastAPI Route": {
    "prefix": "rk-fastapi-route",
    "body": [
      "from fastapi import APIRouter, Depends",
      "from typing import List",
      "",
      "${1:router} = APIRouter(prefix=\"/${2:prefix}\", tags=[\"${3:tag}\"])",
      "",
      "@${1:router}.get(\"/\")",
      "async def get_${2:prefix}():",
      "    \"\"\"${4:Get all items}\"\"\"",
      "    ${5:return []}",
      "",
      "@${1:router}.post(\"/\")",
      "async def create_${2/(.*)/${1:/singularize}/}(item: ${6:Item}):",
      "    \"\"\"${7:Create new item}\"\"\"",
      "    ${8:return item}",
      "$0"
    ],
