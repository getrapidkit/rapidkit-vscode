  "RapidKit Repository": {
    "prefix": "rk-repository",
    "body": [
      "\"\"\"",
      "${1:Repository Name}",
      "\"\"\"",
      "",
      "from typing import Optional, List",
      "from sqlalchemy.orm import Session",
      "from .models import ${2:Model}",
      "",
      "class ${3:RepositoryName}Repository:",
      "    \"\"\"${4:Repository description}\"\"\"",
      "    ",
      "    def __init__(self, db: Session):",
      "        self.db = db",
      "    ",
      "    def find_all(self) -> List[${2:Model}]:",
      "        return self.db.query(${2:Model}).all()",
      "    ",
      "    def find_by_id(self, id: int) -> Optional[${2:Model}]:",
      "        return self.db.query(${2:Model}).filter(${2:Model}.id == id).first()",
      "    ",
      "    def create(self, obj: ${2:Model}) -> ${2:Model}:",
      "        self.db.add(obj)",
      "        self.db.commit()",
      "        self.db.refresh(obj)",
      "        return obj",
      "$0"
    ],
    "description": "RapidKit repository pattern"
  },
