{
  "name": "rapidkit-vscode",
  "displayName": "RapidKit",
  "description": "Scaffold production-ready FastAPI & NestJS APIs with clean architecture",
  "version": "0.8.0",
  "publisher": "rapidkit",
  "icon": "media/icons/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "preview": false,
  "qna": "https://github.com/getrapidkit/rapidkit-vscode/discussions",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other",
    "Formatters"
  ],
  "keywords": [
    "rapidkit",
    "fastapi",
    "nestjs",
    "python",
    "typescript",
    "backend",
    "scaffold",
    "generator",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/getrapidkit/rapidkit-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/getrapidkit/rapidkit-vscode/issues"
  },
  "homepage": "https://getrapidkit.com",
  "license": "MIT",
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rapidkit.createWorkspace",
        "title": "Create Workspace",
        "category": "RapidKit",
        "icon": "$(new-folder)"
      },
      {
        "command": "rapidkit.createProject",
        "title": "Create Project",
        "category": "RapidKit",
        "icon": "$(new-folder)"
      },
      {
        "command": "rapidkit.createFastAPIProject",
        "title": "Create FastAPI Project",
        "category": "RapidKit",
        "icon": "$(symbol-method)"
      },
      {
        "command": "rapidkit.createNestJSProject",
        "title": "Create NestJS Project",
        "category": "RapidKit",
        "icon": "$(symbol-class)"
      },
      {
        "command": "rapidkit.openDocs",
        "title": "Open Documentation",
        "category": "RapidKit",
        "icon": "$(book)"
      },
      {
        "command": "rapidkit.addModule",
        "title": "Add Module",
        "category": "RapidKit",
        "icon": "$(extensions)"
      },
      {
        "command": "rapidkit.refreshProjects",
        "title": "Refresh Projects",
        "category": "RapidKit",
        "icon": "$(refresh)"
      },
      {
        "command": "rapidkit.refreshWorkspaces",
        "title": "Refresh Workspaces",
        "category": "RapidKit",
        "icon": "$(refresh)"
      },
      {
        "command": "rapidkit.selectWorkspace",
        "title": "Select Workspace",
        "category": "RapidKit",
        "icon": "$(check)"
      },
      {
        "command": "rapidkit.addWorkspace",
        "title": "Add Workspace",
        "category": "RapidKit",
        "icon": "$(folder-opened)"
      },
      {
        "command": "rapidkit.removeWorkspace",
        "title": "Remove Workspace",
        "category": "RapidKit",
        "icon": "$(trash)"
      },
      {
        "command": "rapidkit.openWorkspaceFolder",
        "title": "Open in File Explorer",
        "category": "RapidKit",
        "icon": "$(folder-opened)"
      },
      {
        "command": "rapidkit.copyWorkspacePath",
        "title": "Copy Path",
        "category": "RapidKit",
        "icon": "$(copy)"
      },
      {
        "command": "rapidkit.checkWorkspaceHealth",
        "title": "Check Health (Doctor)",
        "category": "RapidKit",
        "icon": "$(pulse)"
      },
      {
        "command": "rapidkit.deleteProject",
        "title": "Delete Project",
        "category": "RapidKit",
        "icon": "$(trash)"
      },
      {
        "command": "rapidkit.openProjectFolder",
        "title": "Open in File Explorer",
        "category": "RapidKit",
        "icon": "$(folder-opened)"
      },
      {
        "command": "rapidkit.copyProjectPath",
        "title": "Copy Path",
        "category": "RapidKit",
        "icon": "$(copy)"
      },
      {
        "command": "rapidkit.discoverWorkspaces",
        "title": "Discover Workspaces",
        "category": "RapidKit",
        "icon": "$(search)"
      },
      {
        "command": "rapidkit.openProjectDashboard",
        "title": "Open Project Dashboard",
        "category": "RapidKit",
        "icon": "$(dashboard)"
      },
      {
        "command": "rapidkit.previewTemplate",
        "title": "Preview Template",
        "category": "RapidKit",
        "icon": "$(eye)"
      },
      {
        "command": "rapidkit.doctor",
        "title": "Run System Check",
        "category": "RapidKit",
        "icon": "$(pulse)"
      },
      {
        "command": "rapidkit.checkSystem",
        "title": "Check RapidKit Installation",
        "category": "RapidKit",
        "icon": "$(checklist)"
      },
      {
        "command": "rapidkit.checkForUpdates",
        "title": "Check for Updates",
        "category": "RapidKit",
        "icon": "$(cloud-download)"
      },
      {
        "command": "rapidkit.showWelcome",
        "title": "Show Welcome",
        "category": "RapidKit",
        "icon": "$(home)"
      },
      {
        "command": "rapidkit.openSetup",
        "title": "Open Setup & Installation",
        "category": "RapidKit",
        "icon": "$(tools)"
      },
      {
        "command": "rapidkit.showLogs",
        "title": "Show Logs",
        "category": "RapidKit",
        "icon": "$(output)"
      },
      {
        "command": "rapidkit.closeLogs",
        "title": "Close Logs",
        "category": "RapidKit",
        "icon": "$(close)"
      },
      {
        "command": "rapidkit.clearLogs",
        "title": "Clear Logs",
        "category": "RapidKit",
        "icon": "$(trash)"
      },
      {
        "command": "rapidkit.projectInit",
        "title": "Install Dependencies",
        "category": "RapidKit",
        "icon": "$(package)"
      },
      {
        "command": "rapidkit.projectDev",
        "title": "Start Dev Server",
        "category": "RapidKit",
        "icon": "$(play)"
      },
      {
        "command": "rapidkit.projectTerminal",
        "title": "Open Terminal",
        "category": "RapidKit",
        "icon": "$(terminal)"
      },
      {
        "command": "rapidkit.projectTest",
        "title": "Run Tests",
        "category": "RapidKit",
        "icon": "$(beaker)"
      },
      {
        "command": "rapidkit.projectBrowser",
        "title": "Open in Browser",
        "category": "RapidKit",
        "icon": "$(globe)"
      },
      {
        "command": "rapidkit.projectStop",
        "title": "Stop Server",
        "category": "RapidKit",
        "icon": "$(debug-stop)"
      },
      {
        "command": "rapidkit.newFile",
        "title": "New File",
        "category": "RapidKit",
        "icon": "$(new-file)"
      },
      {
        "command": "rapidkit.newFolder",
        "title": "New Folder",
        "category": "RapidKit",
        "icon": "$(new-folder)"
      },
      {
        "command": "rapidkit.deleteFile",
        "title": "Delete",
        "category": "RapidKit",
        "icon": "$(trash)"
      },
      {
        "command": "rapidkit.renameFile",
        "title": "Rename",
        "category": "RapidKit",
        "icon": "$(edit)"
      },
      {
        "command": "rapidkit.revealInExplorer",
        "title": "Reveal in Explorer",
        "category": "RapidKit",
        "icon": "$(folder-opened)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rapidkit-explorer",
          "title": "RapidKit",
          "icon": "media/icons/icon-128.png"
        }
      ]
    },
    "views": {
      "rapidkit-explorer": [
        {
          "type": "webview",
          "id": "rapidkitActionsWebview",
          "name": "Quick Actions",
          "icon": "media/icons/icon-128.png",
          "contextualTitle": "Quick Actions"
        },
        {
          "id": "rapidkitWorkspaces",
          "name": "Workspaces",
          "icon": "media/icons/icon-128.png",
          "contextualTitle": "RapidKit Workspaces"
        },
        {
          "id": "rapidkitProjects",
          "name": "Projects",
          "icon": "media/icons/icon-128.png",
          "contextualTitle": "Projects in Workspace"
        },
        {
          "id": "rapidkitModules",
          "name": "Available Modules",
          "icon": "media/icons/icon-128.png",
          "contextualTitle": "RapidKit Modules"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "rapidkitWorkspaces",
        "contents": "No RapidKit workspaces found.\n\nUse the Actions panel above to create your first workspace."
      },
      {
        "view": "rapidkitProjects",
        "contents": "No workspace selected.\n\nSelect a workspace from the Workspaces panel or use Actions to create projects.",
        "when": "!rapidkit:workspaceSelected"
      },
      {
        "view": "rapidkitProjects",
        "contents": "No projects found in this workspace.\n\nUse the Actions panel to create your first project.",
        "when": "rapidkit:workspaceSelected && rapidkit:noProjects"
      },
      {
        "view": "rapidkitModules",
        "contents": "Available modules will appear here once you select a project."
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "rapidkit.createWorkspace"
        },
        {
          "command": "rapidkit.createProject"
        },
        {
          "command": "rapidkit.addModule"
        },
        {
          "command": "rapidkit.refreshProjects",
          "when": "rapidkit:hasProjects"
        },
        {
          "command": "rapidkit.openProjectDashboard",
          "when": "rapidkit:hasProjects"
        },
        {
          "command": "rapidkit.previewTemplate"
        },
        {
          "command": "rapidkit.doctor"
        },
        {
          "command": "rapidkit.checkSystem"
        },
        {
          "command": "rapidkit.checkForUpdates"
        }
      ],
      "view/title": [
        {
          "command": "rapidkit.refreshWorkspaces",
          "when": "view == rapidkitWorkspaces",
          "group": "navigation@1"
        },
        {
          "command": "rapidkit.discoverWorkspaces",
          "when": "view == rapidkitWorkspaces",
          "group": "navigation@2"
        },
        {
          "command": "rapidkit.createProject",
          "when": "view == rapidkitProjects && rapidkit:workspaceSelected",
          "group": "navigation@1"
        },
        {
          "command": "rapidkit.refreshProjects",
          "when": "view == rapidkitProjects",
          "group": "navigation@2"
        }
      ],
      "view/item/context": [
        {
          "command": "rapidkit.selectWorkspace",
          "when": "view == rapidkitWorkspaces && viewItem == workspace",
          "group": "inline@1"
        },
        {
          "command": "rapidkit.checkWorkspaceHealth",
          "when": "view == rapidkitWorkspaces && viewItem == workspace",
          "group": "inline@2"
        },
        {
          "command": "rapidkit.openWorkspaceFolder",
          "when": "view == rapidkitWorkspaces && viewItem == workspace",
          "group": "navigation@1"
        },
        {
          "command": "rapidkit.copyWorkspacePath",
          "when": "view == rapidkitWorkspaces && viewItem == workspace",
          "group": "navigation@2"
        },
        {
          "command": "rapidkit.removeWorkspace",
          "when": "view == rapidkitWorkspaces && viewItem == workspace",
          "group": "z_danger@99"
        },
        {
          "command": "rapidkit.addModule",
          "when": "view == rapidkitProjects && viewItem == project",
          "group": "project@1"
        },
        {
          "command": "rapidkit.openProjectDashboard",
          "when": "view == rapidkitProjects && viewItem == project",
          "group": "project@2"
        },
        {
          "command": "rapidkit.openProjectFolder",
          "when": "view == rapidkitProjects && viewItem == project",
          "group": "navigation@1"
        },
        {
          "command": "rapidkit.copyProjectPath",
          "when": "view == rapidkitProjects && viewItem == project",
          "group": "navigation@2"
        },
        {
          "command": "rapidkit.deleteProject",
          "when": "view == rapidkitProjects && viewItem == project",
          "group": "z_danger@99"
        },
        {
          "command": "rapidkit.projectTerminal",
          "when": "view == rapidkitProjects && viewItem =~ /^project(-running)?$/",
          "group": "inline@1"
        },
        {
          "command": "rapidkit.projectInit",
          "when": "view == rapidkitProjects && viewItem =~ /^project(-running)?$/",
          "group": "inline@2"
        },
        {
          "command": "rapidkit.projectDev",
          "when": "view == rapidkitProjects && viewItem == project",
          "group": "inline@3"
        },
        {
          "command": "rapidkit.projectStop",
          "when": "view == rapidkitProjects && viewItem == project-running",
          "group": "inline@3"
        },
        {
          "command": "rapidkit.projectTest",
          "when": "view == rapidkitProjects && viewItem =~ /^project(-running)?$/",
          "group": "inline@4"
        },
        {
          "command": "rapidkit.projectBrowser",
          "when": "view == rapidkitProjects && viewItem =~ /^project(-running)?$/",
          "group": "inline@5"
        },
        {
          "command": "rapidkit.newFile",
          "when": "view == rapidkitProjects && viewItem =~ /^(project|folder)$/",
          "group": "file@1"
        },
        {
          "command": "rapidkit.newFolder",
          "when": "view == rapidkitProjects && viewItem =~ /^(project|folder)$/",
          "group": "file@2"
        },
        {
          "command": "rapidkit.renameFile",
          "when": "view == rapidkitProjects && viewItem =~ /^(file|folder)$/",
          "group": "file@3"
        },
        {
          "command": "rapidkit.deleteFile",
          "when": "view == rapidkitProjects && viewItem =~ /^(file|folder)$/",
          "group": "file@4"
        },
        {
          "command": "rapidkit.revealInExplorer",
          "when": "view == rapidkitProjects && viewItem =~ /^(project|file|folder)$/",
          "group": "navigation@3"
        }
      ],
      "explorer/context": [
        {
          "command": "rapidkit.createProject",
          "when": "explorerResourceIsFolder",
          "group": "rapidkit@1"
        }
      ]
    },
    "configuration": {
      "title": "RapidKit",
      "properties": {
        "rapidkit.pythonPath": {
          "type": "string",
          "default": "python3",
          "description": "Path to Python executable",
          "scope": "window"
        },
        "rapidkit.defaultKit": {
          "type": "string",
          "default": "fastapi.standard",
          "enum": [
            "fastapi.standard",
            "fastapi.advanced",
            "nestjs.standard",
            "nestjs.advanced"
          ],
          "enumDescriptions": [
            "FastAPI with standard features",
            "FastAPI with advanced features",
            "NestJS with standard features",
            "NestJS with advanced features"
          ],
          "description": "Default kit for new projects"
        },
        "rapidkit.autoRefresh": {
          "type": "boolean",
          "default": true,
          "description": "Automatically refresh project tree when files change"
        },
        "rapidkit.showWelcomeOnStartup": {
          "type": "boolean",
          "default": true,
          "description": "Show welcome page when extension activates"
        },
        "rapidkit.templatePreview": {
          "type": "boolean",
          "default": true,
          "description": "Show template preview before generation"
        },
        "rapidkit.notifications": {
          "type": "string",
          "default": "all",
          "enum": [
            "all",
            "errors",
            "none"
          ],
          "description": "Control notification verbosity"
        },
        "rapidkit.terminal.autoActivate": {
          "type": "boolean",
          "default": true,
          "description": "Automatically activate Python environment in integrated terminal"
        },
        "rapidkit.git.autoInit": {
          "type": "boolean",
          "default": true,
          "description": "Initialize git repository for new projects"
        },
        "rapidkit.dontShowFirstTimeSetup": {
          "type": "boolean",
          "default": false,
          "description": "Don't show first-time setup welcome message"
        },
        "rapidkit.checkForUpdates": {
          "type": "boolean",
          "default": true,
          "description": "Check for RapidKit npm package updates"
        }
      }
    },
    "keybindings": [
      {
        "command": "rapidkit.createWorkspace",
        "key": "ctrl+shift+r ctrl+shift+w",
        "mac": "cmd+shift+r cmd+shift+w"
      },
      {
        "command": "rapidkit.createProject",
        "key": "ctrl+shift+r ctrl+shift+p",
        "mac": "cmd+shift+r cmd+shift+p"
      },
      {
        "command": "rapidkit.addModule",
        "key": "ctrl+shift+r ctrl+shift+m",
        "mac": "cmd+shift+r cmd+shift+m"
      }
    ],
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/python.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "yaml",
        "path": "./snippets/yaml.json"
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": ".rapidkitrc.json",
        "url": "./schemas/rapidkitrc.schema.json"
      },
      {
        "fileMatch": "rapidkit.json",
        "url": "./schemas/rapidkit.schema.json"
      },
      {
        "fileMatch": "**/module.yaml",
        "url": "./schemas/module.schema.json"
      }
    ],
    "yamlValidation": [
      {
        "fileMatch": "**/module.yaml",
        "url": "./schemas/module.schema.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "npm run esbuild-base -- --production && cd webview-ui && npm run build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "esbuild-base": "node esbuild.js",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "webview:install": "cd webview-ui && npm install",
    "webview:build": "cd webview-ui && npm run build",
    "webview:watch": "cd webview-ui && npm run watch",
    "dev": "concurrently \"npm run esbuild-watch\" \"npm run webview:watch\"",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "test": "npm run compile && npm run lint",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:vscode": "vscode-test",
    "sync:modules": "node scripts/sync-modules.mjs",
    "package": "vsce package",
    "publish": "vsce publish",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run format:check && npm test",
    "prepare": "husky"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "execa": "^9.3.1",
    "fs-extra": "^11.2.0",
    "nunjucks": "^3.2.4",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.0",
    "@types/nunjucks": "^3.2.6",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^4.0.7",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^3.6.2",
    "esbuild": "^0.25.12",
    "eslint": "^8.57.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.2.5",
    "typescript": "^5.5.0",
    "vitest": "^4.0.7"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}